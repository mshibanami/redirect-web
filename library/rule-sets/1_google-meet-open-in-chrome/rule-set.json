{
  "bundleID": "io.github.mshibanami.RedirectWebForSafari",
  "formatVersion": "3",
  "kind": "RedirectList",
  "redirects": [
    {
      "appURL": "file:///Applications/Google%20Chrome.app",
      "captureGroupProcesses": [],
      "comments": "This is a rule to open Google Meet links in Google Chrome automatically.",
      "destinationURLPattern": "$0",
      "exampleURLs": [
        "https://meet.google.com/xxx-yyyy-zzz",
        "https://meet.google.com/xxx-yyyy-zzz?pli=1"
      ],
      "excludeURLPatterns": [],
      "kind": "Redirect",
      "sourceURLPattern": {
        "type": "regularExpression",
        "value": "(https://meet.google.com/[a-z]*-[a-z]*-[a-z]*)(\\?.*)?"
      },
      "targetBrowserOptions": {
        "browsers": [
          {
            "type": "chrome"
          }
        ],
        "selectionType": "excluding"
      },
      "title": "Google Meet: Open in Chrome",
      "type": "originalRedirect"
    }
  ]
}

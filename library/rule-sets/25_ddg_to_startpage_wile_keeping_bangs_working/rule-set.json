{
  "bundleID": "io.github.mshibanami.RedirectWebForSafari",
  "formatVersion": "4",
  "kind": "RedirectList",
  "redirects": [
    {
      "captureGroupProcesses": [],
      "comments": "This automatically redirects DuckDuckGo searches to Startpage while keeping **bangs** functional.\n\n**Bangs** are a DuckDuckGo feature that lets you quickly search specific websites using shortcuts (e.g., `!w` for Wikipedia, `!yt` for YouTube). This rule preserves that functionality.\n\nLearn more: https://duckduckgo.com/bangs",
      "commentsOptions": {
        "format": "markdown"
      },
      "destinationURLPattern": "https://www.startpage.com/sp/search?query=$1",
      "exampleURLs": [
        "https://duckduckgo.com/?q=search&kp=-1&kl=us-en",
        "https://www.duckduckgo.com/?q=search&kp=-1&kl=us-en",
        "https://www.duckduckgo.com/?q=!a%20search&kp=-1&kl=us-en"
      ],
      "excludeURLPatterns": [
        {
          "type": "regularExpression",
          "value": "https?://(?:www\\.)?duckduckgo\\.com/\\?(?:.*&)?q=\\!\\w+.*"
        }
      ],
      "kind": "Redirect",
      "sourceURLPattern": {
        "type": "regularExpression",
        "value": "https?://(?:www\\.)?duckduckgo\\.com/\\?(?:.*&)?q=([^&]*).*"
      },
      "targetBrowserOptions": {
        "browsers": [],
        "selectionType": "including"
      },
      "title": "DuckDuckGo â†’ Startpage while keeping bangs working",
      "type": "originalRedirect"
    }
  ]
}
